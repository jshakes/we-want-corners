{
	"manifest_version": 2,
	"name": "We want corners!",
	"description": "",
	"version": "1.0",
	"author": "https://github.com/jshakes/",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["we-want-corners.client.js"],
			"run_at": "document_idle"
		}
	],
	"permissions": [
		"storage",
		"tabs"
	],
	"browser_action": {
		"default_title": "We want corners",
		"default_icon": {
			"38":"icon38.png"
		},  
		"default_popup": "dashboard.html"
	}
}
